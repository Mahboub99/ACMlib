class Point 
{ 
public: 
    int x, y; 
    Point(int a=0, int b=0):x(a),y(b) {} 
}; 
  
int gcd(int a, int b) 
{ 
    if (b == 0) return a;  return gcd(b, a%b); 
} 
 
int getBoundaryCount(Point p,Point q) 
{ 
    
    if (p.x==q.x) 
        return abs(p.y - q.y) - 1; 
    if (p.y == q.y) 
        return abs(p.x-q.x) - 1; 
  
    return gcd(abs(p.x-q.x),abs(p.y-q.y))-1; 
}
